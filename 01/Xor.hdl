/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a = a, b = a, out = nota);
    Nand(a = b, b = b, out = notb);
    Nand(a = a, b = notb, out = notanotb);
    Nand(a = b, b = nota, out = notbnota);
    Nand(a = notbnota, b = notanotb, out = out);
}
